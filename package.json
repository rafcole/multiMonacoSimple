{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fresh": " &&  && node node_modules/y-websocket/bin/server.js &&",
    "kill_node_process_on_port": "lsof -n -i:1234 | grep LISTEN | awk '{ print $2 }' | uniq | xargs -r kill -9",
    "launch_dev": "npm run dev",
    "launch_server": "node node_modules/y-websocket/bin/server.js",
    "delete_last_line_from_env": "sed -i '' -e '$ d'  ./.env",
    "add_uuid_room_to_env": "echo \"ROTATING_ROOM='`uuid`'\" >> ./.env",
    "plzrun": "npm run kill_node_process_on_port && npm run delete_last_line_from_env && npm run add_uuid_room_to_env && npm run launch_server && npm run launch_dev"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.6",
    "@reactivedata/react": "^0.2.1",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "y-monaco": "^0.1.4",
    "y-webrtc": "^10.2.4",
    "y-websocket": "^1.5.0",
    "yjs": "^13.5.44"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "vite": "^4.0.0"
  }
}
